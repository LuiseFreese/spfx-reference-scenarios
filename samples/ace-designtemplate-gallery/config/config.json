{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "benefits-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/benefits/BenefitsAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/benefits/BenefitsAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "ace-design-template-personal-app-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ACEGalleryAppPart/ACEGalleryAppPart.js",
          "manifest": "./src/webparts/ACEGalleryAppPart/ACEGalleryAppPart.manifest.json"
        }
      ]
    },
    "eventschedule-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/eventschedule/EventscheduleAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/eventschedule/EventscheduleAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "faqaccordion-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/faqaccordion/FaqaccordionAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/faqaccordion/FaqaccordionAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "imagecarousel-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/imagecarousel/ImagecarouselAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/imagecarousel/ImagecarouselAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "inventorydetails-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/inventorydetails/InventorydetailsAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/inventorydetails/InventorydetailsAdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BenefitsAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/benefits/loc/{locale}.js",
    "AceDesignTemplatePersonalAppWebPartStrings": "lib/webparts/ACEGalleryAppPart/loc/{locale}.js",
    "EventscheduleAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/eventschedule/loc/{locale}.js",
    "FaqAccordionAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/faqAccordion/loc/{locale}.js",
    "FaqaccordionAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/faqaccordion/loc/{locale}.js",
    "ImagecarouselAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/imagecarousel/loc/{locale}.js",
    "InventorydetailsAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/inventorydetails/loc/{locale}.js"
  }
}
